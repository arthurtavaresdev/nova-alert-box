{
    "name": "arthurtavaresdev/alert-box",
    "description": "Alert box for Laravel Nova",
    "keywords": [
        "laravel",
        "nova",
        "alert-box"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Arthur Tavares",
            "email": "arthurabreu00@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/nova": "^3.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.2.1",
        "pestphp/pest": "^1.20.0",
        "pestphp/pest-plugin-mock": "^1.0",
        "phpstan/phpstan": "^1.1.1",
        "roave/security-advisories": "dev-latest",
        "symfony/var-dumper": "^5.3.10"
    },
    "autoload": {
        "psr-4": {
            "ArthurTavaresDev\\NovaAlertBox\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArthurTavaresDev\\NovaAlertBox\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "nova": {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    }
}
